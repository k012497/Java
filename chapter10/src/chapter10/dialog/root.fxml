<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hBox" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chapter10.dialog.RootController">
   <children>
      <Button mnemonicParsing="false" onAction="#handleOpenFileChooser" text="OpenFile" />
      <Button mnemonicParsing="false" onAction="#handleSaveFileChooser" text="SaveFile" />
      <Button mnemonicParsing="false" onAction="#handleDirectoryChooser" text="Directory" />
      <Button mnemonicParsing="false" onAction="#handlePopup" text="Popup" />
      <Button mnemonicParsing="false" onAction="#handleCustom" text="Custom" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
